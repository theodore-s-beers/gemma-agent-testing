# LLM Provider: google, lmstudio, llamacpp, ollama, openrouter
LLM_PROVIDER=google

# URLs for local LLM services
LMSTUDIO_URL=http://localhost:1234/v1
LLAMACPP_URL=http://localhost:8080/v1
OLLAMA_URL=http://localhost:11434/v1

# Model names for LLM services
# "amazon/nova-2-lite-v1:free" works well for this task
